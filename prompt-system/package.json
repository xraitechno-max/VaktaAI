{
  "name": "@vaktaai/prompt-system",
  "version": "1.0.0",
  "description": "Production-grade dynamic prompt orchestration for VaktaAI EdTech platform with multi-LLM routing, evidence-first RAG, and bilingual support",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "policy",
    "schemas",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "vaktaai",
    "edtech",
    "prompt-engineering",
    "llm",
    "rag",
    "multi-model",
    "hinglish",
    "cbse",
    "jee",
    "neet",
    "education",
    "ai-tutor"
  ],
  "author": "VaktaAI Platform Engineering",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {
    "zod": "^3.22.4",
    "@opentelemetry/api": "^1.7.0",
    "prom-client": "^15.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0",
    "@vitest/coverage-v8": "^1.0.0"
  },
  "peerDependencies": {
    "openai": "^4.0.0"
  },
  "peerDependenciesMeta": {
    "openai": {
      "optional": true
    }
  }
}
